[user]
	name = Vyacheslav Blinov
	email = vblinov@unison.com
[color]
	ui = true

[merge]
	tool = opendiff

[core] 
	whitespace=-indent-with-non-tab,trailing-space,cr-at-eol,tab-in-indent
	excludesfile = ~/.gitexcludes

[diff "localizablestrings"]
	textconv = "iconv -f utf-16 -t utf-8"

[push]
	default = matching
[gui]
	gcwarning = false
[pull]
	rebase = true
[rebase]
	autoStash = true
